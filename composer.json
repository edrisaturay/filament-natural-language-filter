{
    "name": "edrisaturay/filament-natural-language-filter",
    "description": "Natural language filtering for Filament tables and forms",
    "type": "library",
    "license": "MIT",
    "version": "1.0.1",
    "authors": [
        {
            "name": "Edrisa Turay",
            "email": "edrisa@edrisa.com"
        },
        {
            "name": "Hayder Hatem",
            "email": "hayderhatem@example.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "EdrisaTuray\\FilamentNaturalLanguageFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EdrisaTuray\\FilamentNaturalLanguageFilter\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0",
        "filament/filament": "^3.2",
        "openai-php/laravel": "^0.8",
        "illuminate/support": "^11.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-text --coverage-clover=coverage.clover",
        "cs-check": "vendor/bin/php-cs-fixer fix --dry-run --diff",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse --memory-limit=512M",
        "version:patch": "php scripts/version-bump.php patch",
        "version:minor": "php scripts/version-bump.php minor",
        "version:major": "php scripts/version-bump.php major",
        "version:show": "php -r \"echo json_decode(file_get_contents('composer.json'), true)['version'] ?? '0.0.0';\""
    },
    "extra": {
        "laravel": {
            "providers": [
                "EdrisaTuray\\FilamentNaturalLanguageFilter\\FilamentNaturalLanguageFilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
